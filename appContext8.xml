<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">
	
	<!-- Go to Namespaces, check context, ensure that "xmlns:context="http://www.springframework.org/schema/context"
	is present, then save appContext, then create a context tag, with no spaces for <context:annotation....>-->
	<context:annotation-config/>
	
	<bean id = "dairy" class = "AutoWireAnnotation.Dairy"></bean>
	<bean id = "meat" class = "AutoWireAnnotation.Meat"></bean>
	<bean id = "grain" class = "AutoWireAnnotation.Grain"></bean>
	<bean id = "fruit" class = "AutoWireAnnotation.Fruit"></bean>
	<bean id = "vegetable" class = "AutoWireAnnotation.Vegetable"></bean>
	
	<bean id = "meal" class = "AutoWireAnnotation.Meal">		
	</bean>
	
	<bean id = "mealByAnnotation" class = "AutoWireAnnotation.MealwAnnotation">		
	</bean>

</beans>
